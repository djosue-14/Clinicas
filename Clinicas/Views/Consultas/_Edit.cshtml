<div class="card">
    <div class="card-header bg-primary text-white text-center">
        <h3 class="h3">Editar Consulta</h3>
    </div>

    <div class="card-body">
        <div class="card-title lead text-center"></div>
        <form id="formEditConsulta">
            <!--Campos ocultos-->
            <input type="hidden" value="" name="Id" id="Id">
            <input type="hidden" value="" name="Inicio" id="Inicio" />
            <input type="hidden" value="" name="Fin" id="Fin"/>
            <input type="hidden" value="" name="PacienteId" id="PacienteId"/>
            <input type="hidden" value="" name="EstadoConsultaId" id="EstadoConsultaId"/>
            <input type="hidden" value="" name="UsuarioAsignadoId" id="UsuarioAsignadoId"/>
                <div class="row">
                    <div class="col-lg-4 col-md-6 col-sm-12 form-group">
                        <label for="Titulo">Titulo: </label>
                        <input type="text" name="Titulo" id="Titulo" class="form-control">
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-12 form-group">
                        <label for="Fecha">Fecha: </label>
                        <input type="text" name="Fecha" id="Fecha" class="form-control">
                    </div>
                    <div class="col-lg-4 col-md-6 col-sm-12 form-group">
                        <label for="Descripcion">Descripcion: </label>
                        <input type="text" name="Descripcion" id="Descripcion" class="form-control">
                    </div>
                    <div class="col-lg-3 col-md-6 col-sm-12 form-group">
                        <label for="EstadoConsultaId">Estado Consulta: </label>
                        <select class="custom-select" name="EstadoConsultaId" id="EstadoConsultaId">
                        </select>
                    </div>
                    <div class="col-lg-12 col-md-12 co-sm-12 text-right">
                        <button class="btn btn-warning" type="submit"><i class="fa fa-edit"></i> Editar</button>
                    </div>
                </div>
        </form>        
    </div>
</div>